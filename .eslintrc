{
  "extends": "airbnb",
  "env": {
      "browser": true,
      "es6": true
  },
  "rules": {
      "comma-dangle": 0,
      "no-console": ["error", { "allow": ["warn", "error"] }],
      "indent": [2, 4, {"SwitchCase": 1}],
      "react/jsx-indent-props": [2, 4],
      "react/jsx-indent": [2, 4],
      "linebreak-style": 0,
      "react/require-default-props": 0
  },
  "globals": {
        "window": false,
        "document": false,
        "jest": false,
        "after": false,
        "afterEach": false,
        "afterAll": false,
        "before": false,
        "beforeEach": false,
        "beforeAll": false,
        "describe": false,
        "expect": false,
        "it": false
    }
}
